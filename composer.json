{
	"name": "wwagner/wwonepagetemplate",
	"type": "typo3-cms-extension",
	"description": "This is a small demo one pager template. Take a look at the README for installation instructions.",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Wolfgang Wagner",
			"role": "Developer"
		}
	],
	"version": "3.0.0",
	"require": {
		"typo3/cms-core": "^13.4",
		"helhum/typo3-console": "^8.2"
	},
	"autoload": {
		"psr-4": {
			"Wwagner\\Wwonepagetemplate\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Wwagner\\Wwonepagetemplate\\Tests\\": "Tests"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "wwonepagetemplate"
		}
	}
}
